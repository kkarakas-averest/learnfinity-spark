  "notes": "IMPORTANT: For Vercel deployment, add these environment variables in Vercel project settings: VITE_SUPABASE_URL, VITE_SUPABASE_ANON_KEY, VITE_SUPABASE_SERVICE_KEY",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "copy-pdf-worker": "cp node_modules/pdfjs-dist/build/pdf.worker.min.mjs public/",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:agents": "node src/agents/testing/run-agent-tests.js",
    "db:create-exec-sql": "PGPASSWORD=$SUPABASE_DB_PASSWORD psql -h $SUPABASE_DB_HOST -p $SUPABASE_DB_PORT -d $SUPABASE_DB_NAME -U $SUPABASE_DB_USER -f src/db/create-exec-sql-function.sql",
    "db:apply-schema": "node src/db/apply-foundation-schema.js",
    "db:setup": "npm run db:create-exec-sql && npm run db:apply-schema",
    "db:seed": "tsx src/utils/database/seedDatabase.ts",
    "db:check-config": "tsx src/utils/database/checkSupabaseConfig.ts",
    "db:apply-hr-schema": "dotenv -e .env npx tsx src/utils/applyHrSchema.ts",
    "db:create-user-mappings": "dotenv -e .env npx tsx src/utils/createEmployeeUserMappings.ts",
    "test:db": "NODE_ENV=test pnpm run db:migrate && vitest run --config ./vitest.config.db.ts",
    "test:db:ui": "NODE_ENV=test pnpm run db:migrate && vitest --ui --config ./vitest.config.db.ts"
  },
