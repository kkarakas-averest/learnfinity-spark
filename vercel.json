{
  "version": 2,
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "framework": "vite",
  "installCommand": "pnpm install",
  "outputDirectory": "dist",
  "github": {
    "enabled": true,
    "silent": false
  },
  "functions": {
    "api/hr/courses/[courseId]/enrollment.ts": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/hr/employee-courses.ts": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/chat/conversation.ts": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/hr/positions-with-requirement-counts.ts": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/hr/positions-test.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/skills/position-requirements.ts": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/skills/taxonomy-search.ts": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/ai/suggest-skills.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,OPTIONS,PATCH,DELETE,POST,PUT",
        "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization"
      },
      "continue": true
    },
    {
      "src": "/api/hr/courses/(?<courseId>[^/]+)/enrollment",
      "dest": "/api/hr/courses/[courseId]/enrollment.ts"
    },
    {
      "src": "/api/hr/employee-courses",
      "dest": "/api/hr/employee-courses.ts"
    },
    {
      "src": "/api/chat/conversation",
      "dest": "/api/chat/conversation.ts"
    },
    {
      "src": "/api/hr/positions-with-requirement-counts",
      "dest": "/api/hr/positions-with-requirement-counts.ts"
    },
    {
      "src": "/api/hr/positions-test",
      "dest": "/api/hr/positions-test.js"
    },
    {
      "src": "/api/skills/position-requirements",
      "dest": "/api/skills/position-requirements.ts"
    },
    {
      "src": "/api/skills/taxonomy-search",
      "dest": "/api/skills/taxonomy-search.ts"
    },
    {
      "src": "/api/ai/suggest-skills",
      "dest": "/api/ai/suggest-skills.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "env": {
    "VITE_SUPABASE_URL": "${VITE_SUPABASE_URL}",
    "VITE_SUPABASE_ANON_KEY": "${VITE_SUPABASE_ANON_KEY}",
    "VITE_SUPABASE_SERVICE_KEY": "${VITE_SUPABASE_SERVICE_KEY}",
    "SUPABASE_URL": "${VITE_SUPABASE_URL}",
    "SUPABASE_ANON_KEY": "${VITE_SUPABASE_ANON_KEY}",
    "SUPABASE_SERVICE_KEY": "${VITE_SUPABASE_SERVICE_KEY}",
    "NEXT_PUBLIC_SUPABASE_URL": "${VITE_SUPABASE_URL}",
    "SUPABASE_SERVICE_ROLE_KEY": "${VITE_SUPABASE_SERVICE_KEY}",
    "GROQ_API_KEY": "${GROQ_API_KEY}",
    "NODE_ENV": "production",
    "VERCEL_DEPLOYMENT": "true",
    "DEPLOYMENT_TIMESTAMP": "2025-04-21T10-15-03-825Z"
  }
}