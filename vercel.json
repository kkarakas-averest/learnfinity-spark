{
  "version": 2,
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "framework": "vite",
  "installCommand": "pnpm install",
  "outputDirectory": "dist",
  "github": {
    "enabled": true,
    "silent": false
  },
  "regions": ["sfo1"],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/api/hr/courses/(?<courseId>[^/]+)/enrollment",
      "dest": "/api/hr/courses/[courseId]/enrollment.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "memory": 1024,
      "maxDuration": 10
    },
    {
      "src": "/api/hr/employee-courses",
      "dest": "/api/hr/employee-courses.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "memory": 1024,
      "maxDuration": 10
    },
    {
      "src": "/api/hr/employee-goals",
      "dest": "/api/hr/employee-goals.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "memory": 1024,
      "maxDuration": 30
    },
    {
      "src": "/api/hr/employees",
      "dest": "/api/hr/employees.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "memory": 1024,
      "maxDuration": 30
    },
    {
      "src": "/api/hr/departments",
      "dest": "/api/hr/departments.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/hr/positions",
      "dest": "/api/hr/positions.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/hr/positions-with-requirement-counts",
      "dest": "/api/hr/positions-with-requirement-counts.ts",
      "methods": ["GET", "OPTIONS"],
      "memory": 1024,
      "maxDuration": 10
    },
    {
      "src": "/api/skills/position-requirements",
      "dest": "/api/skills/position-requirements.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "memory": 1024,
      "maxDuration": 10
    },
    {
      "src": "/api/skills/position-requirements/apply-suggestions",
      "dest": "/api/skills/position-requirements/apply-suggestions.ts",
      "methods": ["POST", "OPTIONS"],
      "memory": 1024,
      "maxDuration": 15
    },
    {
      "src": "/api/skills/taxonomy-search",
      "dest": "/api/skills/taxonomy-search.ts",
      "methods": ["GET", "POST", "OPTIONS"],
      "memory": 1024,
      "maxDuration": 10
    },
    {
      "src": "/api/skills/employee-skills",
      "dest": "/api/skills/employee-skills.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/skills/gap-analysis",
      "dest": "/api/skills/gap-analysis.ts",
      "methods": ["GET", "POST", "OPTIONS"],
      "memory": 1024,
      "maxDuration": 10
    },
    {
      "src": "/api/skills/course-generation",
      "dest": "/api/skills/course-generation.ts",
      "methods": ["POST", "OPTIONS"],
      "memory": 2048,
      "maxDuration": 60
    },
    {
      "src": "/api/ai/suggest-skills",
      "dest": "/api/ai/suggest-skills.ts",
      "methods": ["POST", "OPTIONS"],
      "memory": 1024,
      "maxDuration": 15
    },
    {
      "src": "/api/ai/regenerate-content",
      "dest": "/api/ai/regenerate-content.ts",
      "methods": ["POST", "OPTIONS"],
      "memory": 1024,
      "maxDuration": 60
    },
    {
      "src": "/api/chat/conversation",
      "dest": "/api/chat/conversation.ts",
      "methods": ["POST", "OPTIONS"],
      "memory": 2048,
      "maxDuration": 60
    },
    {
      "src": "/api/chat/model-settings",
      "dest": "/api/chat/model-settings.ts",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/api/course-generation/generate",
      "dest": "/api/course-generation/generate.ts",
      "methods": ["POST", "OPTIONS"],
      "memory": 3008,
      "maxDuration": 300
    },
    {
      "src": "/api/employee",
      "dest": "/api/employee.js",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/$1"
    },
    {
      "src": "/api/profile/set-preferences",
      "dest": "/api/profile/set-preferences.ts",
      "methods": ["POST", "OPTIONS"],
      "memory": 1024,
      "maxDuration": 10
    },
    {
      "src": "/api/sentry",
      "dest": "/api/sentry",
      "memory": 1024,
      "maxDuration": 10
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "env": {
    "VITE_SUPABASE_URL": "${VITE_SUPABASE_URL}",
    "VITE_SUPABASE_ANON_KEY": "${VITE_SUPABASE_ANON_KEY}",
    "VITE_SUPABASE_SERVICE_KEY": "${VITE_SUPABASE_SERVICE_KEY}",
    "SUPABASE_URL": "${VITE_SUPABASE_URL}",
    "SUPABASE_ANON_KEY": "${VITE_SUPABASE_ANON_KEY}",
    "SUPABASE_SERVICE_KEY": "${VITE_SUPABASE_SERVICE_KEY}",
    "NEXT_PUBLIC_SUPABASE_URL": "${VITE_SUPABASE_URL}",
    "SUPABASE_SERVICE_ROLE_KEY": "${VITE_SUPABASE_SERVICE_KEY}",
    "GROQ_API_KEY": "${GROQ_API_KEY}",
    "NODE_ENV": "production",
    "VERCEL_DEPLOYMENT": "true",
    "DEPLOYMENT_TIMESTAMP": "2025-04-21T10-15-03-825Z"
  }
}