{
  "version": 2,
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "framework": "vite",
  "outputDirectory": "dist",
  "installCommand": "pnpm install",
  "github": {
    "enabled": true,
    "silent": false
  },
  "regions": ["sfo1"],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/api/(.*)",
      "headers": {
        "Cache-Control": "no-store, max-age=0",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      },
      "dest": "/api/$1",
      "continue": false
    },
    {
      "src": "/(.*\\.(js|css|map|ico|json|png|svg|jpg|jpeg|gif|pdf|woff|woff2|ttf|otf)$)",
      "dest": "/$1"
    },
    {
      "src": "/test.html",
      "dest": "/test.html"
    },
    {
      "src": "/vercel.html",
      "dest": "/vercel.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/hr/courses/[courseId]/enrollment.ts": {
      "memory": 1024,
      "maxDuration": 10,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/hr/employee-courses.ts": {
      "memory": 1024,
      "maxDuration": 10,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/hr/employees/[employeeId]/index.ts": {
      "memory": 1024,
      "maxDuration": 30,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/chat/conversation.ts": {
      "memory": 2048,
      "maxDuration": 60,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/hr/positions-with-requirement-counts.ts": {
      "memory": 1024,
      "maxDuration": 10,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/skills/position-requirements.ts": {
      "memory": 1024,
      "maxDuration": 10,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/skills/position-requirements/apply-suggestions.ts": {
      "memory": 1024,
      "maxDuration": 15,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/skills/taxonomy-search.ts": {
      "memory": 1024,
      "maxDuration": 10,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/ai/suggest-skills.ts": {
      "memory": 1024,
      "maxDuration": 15,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/ai/regenerate-content.ts": {
      "memory": 1024,
      "maxDuration": 60,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/courses/generate.ts": {
      "memory": 3008,
      "maxDuration": 300,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/courses/bulk-generate.ts": {
      "memory": 3008,
      "maxDuration": 300,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/courses/publish.ts": {
      "memory": 1024,
      "maxDuration": 60,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/uploads/index.ts": {
      "memory": 1024,
      "maxDuration": 30,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/groq-test.js": {
      "memory": 1024,
      "maxDuration": 30,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/hr/db-permissions-check.js": {
      "memory": 1024,
      "maxDuration": 10,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/hr/debug-api-health.js": {
      "memory": 1024,
      "maxDuration": 10,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 10,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/runtime-env.ts": {
      "memory": 1024,
      "maxDuration": 10,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/debug-env.ts": {
      "memory": 1024,
      "maxDuration": 10,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/learner/dashboard.js": {
      "memory": 1024,
      "maxDuration": 30,
      "runtime": "@vercel/node@4.0.0"
    },
    "api/hr/positions-test.js": {
      "memory": 1024,
      "maxDuration": 10,
      "runtime": "@vercel/node@4.0.0"
    }
  },
  "env": {
    "VITE_SUPABASE_URL": "${VITE_SUPABASE_URL}",
    "VITE_SUPABASE_ANON_KEY": "${VITE_SUPABASE_ANON_KEY}",
    "VITE_SUPABASE_SERVICE_KEY": "${VITE_SUPABASE_SERVICE_KEY}",
    "SUPABASE_URL": "${VITE_SUPABASE_URL}",
    "SUPABASE_ANON_KEY": "${VITE_SUPABASE_ANON_KEY}",
    "SUPABASE_SERVICE_KEY": "${VITE_SUPABASE_SERVICE_KEY}",
    "NEXT_PUBLIC_SUPABASE_URL": "${VITE_SUPABASE_URL}",
    "SUPABASE_SERVICE_ROLE_KEY": "${VITE_SUPABASE_SERVICE_KEY}",
    "GROQ_API_KEY": "${GROQ_API_KEY}",
    "NODE_ENV": "production",
    "VERCEL_DEPLOYMENT": "true",
    "DEPLOYMENT_TIMESTAMP": "2025-04-21T10-15-03-825Z"
  }
}